{
  "manifest_version":2,
  "name":"Naver Land Scraper",
  "description":"Naver Land Scraper",
  "version":"1.2",
  "icons":{
    "48":"imgs/icon.png"
  },
  "browser_action":{
      "default_icon":"imgs/icon.png",
      "default_popup":"popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions":[
      "<all_urls>",
      "tabs",
      "http://*/",
      "https://*/"
  ]
}
